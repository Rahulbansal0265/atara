<% if(session.type==1){ %>
    <%-include('../partials/header')%>
        <%}else{%>
            <%- include ("../subAdmin/partials/header") %>
                <%}%>
                    <!-- /main navbar -->

                    <!-- Page container -->
                    <div class="page-container">

                        <!-- Page content -->
                        <div class="page-content">

                            <!-- Main sidebar -->
                            <% if(session.type==1){ %>
                                <%-include('../partials/sidebar')%>
                                    <%}else{%>
                                        <%- include ("../subAdmin/partials/sidebar") %>
                                            <%}%>
                                                <!-- /main sidebar -->

                                                <!-- Main content -->
                                                <div class="content-wrapper">

                                                    <!-- Page header -->
                                                    <div class="page-header page-header-default">
                                                        <div class="page-header-content">
                                                            <div class="page-title">
                                                                <h4>
                                                                    <a href="javascript:history.go(-1);"><i
                                                                            class="icon-arrow-left52 position-left"></i></a><span
                                                                        class="text-semibold">Home</span> -
                                                                    Professionals
                                                                </h4>
                                                            </div>

                                                        </div>

                                                    </div>
                                                    <!-- /page header -->

                                                    <!-- Content area -->
                                                    <div class="content">

                                                        <div class="panel panel-flat">

                                                            <div class="panel-body">

                                                                <form id="subcategoryvalidation" class="form-horizontal"
                                                                    action="/updatepro/<%-editpro.id%>"
                                                                    enctype="multipart/form-data" method="post">
                                                                    <fieldset class="content-group">
                                                                        <legend class="text-bold">Edit Professionals
                                                                        </legend>

                                                                        <div class="form-group">
                                                                            <label class="control-label col-lg-2">First
                                                                                Name<span
                                                                                    class="text-danger">*</span></label>
                                                                            <div class="col-lg-10">
                                                                                <input type="text" name="full_name"
                                                                                    id="full_name" class="form-control"
                                                                                    value="<%-editpro.full_name%>">
                                                                            </div>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <label class="control-label col-lg-2">Last
                                                                                Name<span
                                                                                    class="text-danger">*</span></label>
                                                                            <div class="col-lg-10">
                                                                                <input type="text" name="lastname"
                                                                                    id="lastname" class="form-control"
                                                                                    value="<%-editpro.lastname%>">
                                                                            </div>
                                                                        </div>
                                                                        <!-- <div class="form-group">
                                                    <label class="control-label col-lg-2">Payment<span
                                                            class="text-danger">*</span></label>
                                                    <div class="col-lg-10">
                                                        <input type="number" name="payment" id="payment" class="form-control"
                                                           value="<%-editpro.payment%>">
                                                    </div>
                                                </div> -->
                                                                        <div class="form-group">
                                                                            <label
                                                                                class="control-label col-lg-2">DOB<span
                                                                                    class="text-danger"></span></label>
                                                                            <div class="col-lg-10">
                                                                                <input type="date" name="birthdate"
                                                                                    id="txtDate" class="form-control"
                                                                                    value="<%-editpro.birthdate%>">
                                                                            </div>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <label
                                                                                class="control-label col-lg-2">Email<span
                                                                                    class="text-danger">*</span></label>
                                                                            <div class="col-lg-10">
                                                                                <input type="text" name="email"
                                                                                    id="email" class="form-control"
                                                                                    value="<%-editpro.email%>"
                                                                                    Read-only>
                                                                            </div>
                                                                        </div>

                                                                        <div class="form-group">
                                                                            <label
                                                                                class="control-label col-lg-2">Phone<span
                                                                                    class="text-danger">*</span></label>
                                                                            <div class="col-lg-10">
                                                                                <input type="number" name="phone"
                                                                                    id="phone" class="form-control"
                                                                                    value="<%-editpro.phone%>"
                                                                                    minlength="6" maxlength="16">
                                                                            </div>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <label
                                                                                class="control-label col-lg-2">Gender<span
                                                                                    class="text-danger"></span></label>
                                                                            <div class="col-lg-10">

                                                                                <input type="radio" name="gender"
                                                                                    value="1" <%=(editpro.gender=="1"
                                                                                    )? "checked" : "" %> >Male
                                                                                <input type="radio" name="gender"
                                                                                    value="2" <%=(editpro.gender=="2"
                                                                                    )? "checked" : "" %> >Female
                                                                            </div>
                                                                        </div>

                                                                        <div class="text-right">

                                                                            <button type="submit"
                                                                                class="btn btn-primary">Update</button>
                                                                            <a href="/admin/professionals"
                                                                                style="background-color: #8080808c; color: black;"
                                                                                type="submit" class="btn btn-secondary">
                                                                                Back
                                                                            </a>
                                                                        </div>
                                                                    </fieldset>
                                                                </form>
                                                            </div>
                                                        </div>



                                                        <!-- Footer -->
                                                        <%- include('../partials/footer') %>
                                                            <!-- /footer -->

                                                    </div>
                                                    <!-- /content area -->

                                                </div>
                                                <!-- /main content -->

                        </div>
                        <!-- /page content -->

                    </div>
                    <!-- /page container -->

                    </body>

                    </html>
                    <script>
                        $(document).ready(function () {

                            jQuery.validator.addMethod("noSpace", function (value, element) {
                                return value == '' || value.trim().length != 0;
                            }, "Space shouldn't be allowed");


                            $("#subcategoryvalidation").validate({

                                ignore: [],
                                // Specify validation rules
                                rules: {


                                    full_name: {
                                        required: true,
                                        minlength: 3,
                                        noSpace: true
                                    },
                                    lastname: {
                                        required: true,
                                        minlength: 3,
                                        noSpace: true
                                    },
                                    // payment: {
                                    // required: true,
                                    // noSpace : true
                                    // },

                                    email: {
                                        required: true,
                                        noSpace: true,
                                        email: true
                                    },
                                    phone: {
                                        required: true,
                                        minlength: 6,
                                        maxlength: 16,
                                        noSpace: true
                                    },
                                    password: {
                                        required: true,
                                        minlength: 6,
                                        // maxlength: 16,
                                        noSpace: true
                                    },
                                    // gender: {
                                    //    required: true,
                                    //    minlength: 3,
                                    //    noSpace : true
                                    //    },
                                    // birthdate: {
                                    // required: true,
                                    // noSpace : true
                                    // },


                                },
                                messages: {
                                    //  name: {
                                    //  required: "Please enter name",
                                    // },      

                                    full_name: {
                                        required: " Please enter first name",
                                        accept: "Please enter a vaild first name "
                                    },
                                    lastname: {
                                        required: " Please enter last name",
                                        accept: "Please enter a vaild last name "
                                    },
                                    email: {
                                        required: " Please enter email",
                                        accept: "Please enter a vaild email "
                                    },
                                    // birthdate: {
                                    //    required: " Please enter birthdate",
                                    //    accept: "Please enter a vaild birthdate "
                                    // },
                                    password: {
                                        required: " Please enter password",
                                        accept: "Please enter a vaild password "
                                    },
                                    phone: {
                                        required: " Please enter phone",
                                        accept: "Please enter a vaild phone "
                                    },




                                },

                            });

                            $(function () {
                                var dtToday = new Date();

                                var month = dtToday.getMonth() + 1;
                                var day = dtToday.getDate();
                                var year = dtToday.getFullYear();
                                if (month < 10)
                                    month = '0' + month.toString();
                                if (day < 10)
                                    day = '0' + day.toString();

                                var maxDate = year + '-' + month + '-' + day;
                                // alert(maxDate);
                                $('#txtDate').attr('max', maxDate);
                            });
                        });
                    </script>