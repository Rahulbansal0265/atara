    <%-include('../partials/header')%>
  
                    <div class="page-container">

                        <!-- Page content -->
                        <div class="page-content">

                            <!-- Main sidebar -->

                          
                                <%-include('../partials/sidebar')%>
                                 

                                                <!-- /main sidebar -->

                                                <!-- Main content -->
                                                <div class="content-wrapper">

                                                    <!-- Page header -->
                                                    <div class="page-header page-header-default">
                                                        <div class="page-header-content">
                                                            <div class="page-title">
                                                                <h4> <a href="javascript:history.go(-1);"><i
                                                                            class="icon-arrow-left52 position-left"></i></a><span
                                                                        class="text-semibold">Home</span> - Influencers
                                                                </h4>
                                                            </div>

                                                        </div>

                                                    </div>
                                                    <!-- /page header -->

                                                    <!-- Content area -->
                                                    <div class="content">

                                                        <div class="panel panel-flat">
                                                            <div class="panel-body">
                                                                <form class="form-horizontal" id="basic-form" action="#"
                                                                    enctype="multipart/form-data" method="post">
                                                                    <fieldset class="content-group">
                                                                        <legend class="text-bold">
                                                                            View Influencers
                                                                        </legend>

                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">Profile Image</div>
                                                                            <div class="col-lg-10">
                                                                                <% if(users.image){%>
                                                                                    <a href="/images/<%= users.image %>"
                                                                                        target="_blank"><img
                                                                                            style="width:100px; height:100px;"
                                                                                            src="/images/<%= users.image %>"
                                                                                            alt="<%= users.image %>"
                                                                                            srcset=""></a>
                                                                                    <%}else{%>
                                                                                        <a href="/assets/images/download.jpg"
                                                                                            target="_blank"> <img
                                                                                                style="width:100px; height:100px;"
                                                                                                src="/assets/images/download.jpg"
                                                                                                alt="<%= users.image %>"
                                                                                                srcset=""></a>
                                                                                        <%}%>
                                                                            </div>
                                                                        </div>





                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">Full Name</div>

                                                                            <div class="col-lg-10">
                                                                                <%=users.full_name%>
                                                                            </div>
                                                                        </div>

                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">Email</div>

                                                                            <div class="col-lg-10">
                                                                                <%=users.email%>
                                                                            </div>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">Phone Number</div>

                                                                            <div class="col-lg-10">
                                                                                <%=users.phone%>
                                                                            </div>
                                                                        </div>

                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">Date of Birth</div>

                                                                            <div class="col-lg-10">
                                                                                <%=users.birthdate%>
                                                                            </div>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">Instagram User Name
                                                                            </div>

                                                                            <div class="col-lg-10">
                                                                                <%=users.insta_id%>
                                                                            </div>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">Tiktok User Name</div>

                                                                            <div class="col-lg-10">
                                                                                <%=users.tiktok_id%>
                                                                            </div>
                                                                        </div>

                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">Gender</div>

                                                                            <div class="col-lg-10">
                                                                                <% if(users.gender==1) {%>
                                                                                    Male
                                                                                    <%} else if(users.gender==2) {%>
                                                                                        Female
                                                                                        <%} else{%>
                                                                                            Other
                                                                                            <%}%>
                                                                            </div>
                                                                        </div>


                                                                    </fieldset>

                                                                </form>
                                                            </div>
                                                        </div>



                                                        <div class="panel panel-flat">
                                                            <div class="panel-body">
                                                                <form class="form-horizontal" id="basic-form" action="#" enctype="multipart/form-data" method="post">
                                                                    <fieldset class="content-group">
                                                                        <legend class="text-bold">
                                                                            Statistics
                                                                        </legend>
                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">Male</div>
                                                                            <div class="col-lg-10">
                                                                                <%= genderPercentage.male %>%
                                                                            </div>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">Female</div>
                                                                            <div class="col-lg-10">
                                                                                <%= genderPercentage.female %>%
                                                                            </div>
                                                                        </div>
                                                        
                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">age less than 18</div>
                                                                            <div class="col-lg-10">
                                                                                <%= agePercentage.lessThanEighteen %>%
                                                                            </div>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">age 18 to 24</div>
                                                                            <div class="col-lg-10">
                                                                                <%= agePercentage.inBetweenEighteenToTwentyFour %>%
                                                                            </div>
                                                                        </div>
                                                        
                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">age 25 to 34</div>
                                                                            <div class="col-lg-10">
                                                                                <%= agePercentage.inBetweenTwentyFiveToThirtyFour %>%
                                                                            </div>
                                                                        </div>
                                                        
                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">age 35 to 44</div>
                                                                            <div class="col-lg-10">
                                                                                <%= agePercentage.inBetweenThirtyFiveToFourtyFour %>%
                                                                            </div>
                                                                        </div>
                                                        
                                                                        <div class="form-group">
                                                                            <div class="col-lg-2">More than 44</div>
                                                                            <div class="col-lg-10">
                                                                                <%= agePercentage.moreThanFourtyFour %>%
                                                                            </div>
                                                                        </div>
                                                        
                                                                    </fieldset>
                                                                </form>
                                                            </div>
                                                            <div class="text-right">
                                                        
                                                                <a href="/userslisting" style="background-color: #8080808c; color: black;" type="submit"
                                                                    class="btn btn-secondary">
                                                                    Back
                                                                </a>
                                                            </div>
                                                        </div>



                                                        <!-- Footer -->

                                                       
                                                            <%-include('../partials/footer')%>
                                                               

                                                    </div>
                                                    

                                                    
                                                </div>
                        </div>
                    </div>
                    </body>
                    </html>