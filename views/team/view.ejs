

    <%-include('../partials/header')%>

    <!-- /main navbar -->

    <!-- Page container -->
    <div class="page-container">

        <!-- Page content -->
        <div class="page-content">

            <!-- Main sidebar -->
            <%-include('../partials/sidebar')%>
                <!-- /main sidebar -->

                <!-- Main content -->
                <div class="content-wrapper">

                    <!-- Page header -->
                    <div class="page-header page-header-default">
                        <div class="page-header-content">
                            <div class="page-title">
                                <h4> <a href="javascript:history.go(-1);"><i class="icon-arrow-left52 position-left"></i></a><span
                                         class="text-semibold">Home</span> - Team</h4>
                            </div>

                        </div>

                    </div>
                    <!-- /page header -->

                    <!-- Content area -->
                    <div class="content">

                        <div class="panel panel-flat">
                            

                            <div class="panel-body">

                                
                                <form class="form-horizontal" id="basic-form" action="#" enctype="multipart/form-data"
                                    method="post">
                                    <fieldset class="content-group">
                                        <legend class="text-bold">

                                            View team
                                            
                                          </legend>
                                          
                                          

                                         
                                         <div class="form-group">
                                            <div class="col-lg-2" style="font-weight:bold;">Subadmin Name</div>
                                 
                                            <div class="col-lg-10">
                                                <%=business.subadmin.full_name %>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-2" style="font-weight:bold;">Subadmin Email</div>
                                 
                                            <div class="col-lg-10">
                                                <%=business.subadmin.email %>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-2" style="font-weight:bold;">Subadmin Image</div>
                                 
                                            <div class="col-lg-10">
                                                <img style=" width:100px; height:100px;"
                                    src="/images/<%= business.subadmin.image %>" alt="<%= business.subadmin.image %>" srcset="">
                                            </div>
                                        </div>



                                        <div class="form-group">
                                            <div class="col-lg-2" style="font-weight:bold;">User Name</div>
                                 
                                            <div class="col-lg-10">
                                                <%=business.full_name %>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-2" style="font-weight:bold;">User Email</div>
                                 
                                            <div class="col-lg-10">
                                                <%=business.email %>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-2" style="font-weight:bold;">User Image</div>
                                 
                                            <div class="col-lg-10">
                                                <img style=" width:100px; height:100px;"
                                    src="/images/<%= business.image %>" alt="<%= business.subadmin.image %>" srcset="">
                                            </div>
                                        </div>
                                        
                                         
                                       
                                        

                                       


                                     
                                 <div class="text-right">

                                            <a href="javascript:history.go(-1);" class="m-5 btn btn-primary">Back</a>
                                        </div>
                                </form>
                            </div>
                        </div>



                        <!-- Footer -->
                        <%-include('../partials/footer')%>
                            <!-- /footer -->

                    </div>
                    <!-- /content area -->

                </div>
                <!-- /main content -->

        </div>
        <!-- /page content -->

    </div>
    <!-- /page container -->

    </body>

    </html>