  <style>
    .error {
      color: rgb(134, 13, 13);
      background-color: #fff;
    }

    button.swal2-confirm.btn.btn-danger {
      margin-left: 5px;
    }

    body {
      background-image: url(images/images1.jpg);
      background-size: cover;
    }

    .navbar.navbar-inverse {
      background-color: rgb(8, 27, 41);
    }

    .sidebar.sidebar-main {
      background-color: rgba(0, 0, 0, 0.898);
    }

    input.control-label.col-lg-3 {
      margin-left: -10px;
    }


    .logo {
      background-color: white;
    }

    .panel.bg-black-400 {
      margin-left: 35px;
      width: 237px;

      color: black;
    }

    .container {
      width: 700px;
    }

    p.pera_end {
      display: flex;
      justify-content: end;
      padding: 4px 0px;
      margin: 0px;
    }

    .pera_end input {
      margin: 0;
      margin-right: 6px;
    }


    .dataTables_filter input {
      border-radius: 25px !important;
    }

    .custom-switch-input:checked~.custom-switch-indicator {
      background: #6777ef;
    }

    .custom-switch-input:checked~.custom-switch-indicator:before {
      left: calc(1rem + 1px);
    }

    .custom-switch-input {
      position: absolute;
      z-index: -1;
      opacity: 0;
    }

    .custom-switch-indicator:before {
      content: '';
      position: absolute;
      height: calc(1.25rem - 4px);
      width: calc(1.25rem - 4px);
      top: 1px;
      left: 1px;
      background: #fff;
      border-radius: 50%;
      transition: .3s left;
    }

    .custom-switch-indicator {
      display: inline-block;
      height: 1.25rem;
      width: 2.25rem;
      background: #e9ecef;
      border-radius: 50px;
      position: relative;
      vertical-align: bottom;
      border: 1px solid rgba(0, 40, 100, 0.12);
      transition: .3s border-color, .3s background-color;
    }
  </style>
</head>

