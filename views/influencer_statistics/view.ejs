<%-include('../partials/header')%>

    <!-- /main navbar -->

    <!-- Page container -->
    <div class="page-container">

        <!-- Page content -->
        <div class="page-content">

            <!-- Main sidebar -->
            <%-include('../partials/sidebar')%>
                <!-- /main sidebar -->

                <!-- Main content -->
                <div class="content-wrapper">

                    <!-- Page header -->
                    <div class="page-header page-header-default">
                        <div class="page-header-content">
                            <div class="page-title">
                                <h4> <a href="javascript:history.go(-1);"><i
                                            class="icon-arrow-left52 position-left"></i></a><span
                                        class="text-semibold">Influencer Detail</span></h4>
                            </div>
                        </div>
                    </div>
                    <!-- /page header -->

                    <!-- Content area -->
                    <div class="content">
                        <div class="panel panel-flat">
                            <div class="panel-body">
                                <form class="form-horizontal" id="basic-form" action="#" enctype="multipart/form-data"
                                    method="post">
                                    <fieldset class="content-group">
                                        <legend class="text-bold">
                                            View Influencer statistics
                                        </legend>
                                        <div class="form-group">
                                            <div class="col-lg-2">Average View</div>
                                            <div class="col-lg-10">
                                                <%=infliencerStatisticsArr.view_avg%>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-lg-2">Male</div>

                                            <div class="col-lg-10">
                                                <%=infliencerStatisticsArr.male%>%
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-lg-2">Female</div>

                                            <div class="col-lg-10">
                                                <%=infliencerStatisticsArr.female%>%
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-2">Less than 18</div>

                                            <div class="col-lg-10">
                                                <%=infliencerStatisticsArr.less_than_eighteen%>%
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-2">18 To 24</div>

                                            <div class="col-lg-10">
                                                <%=infliencerStatisticsArr.eighteen_to_twentyFour%>%
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-lg-2">25 To 34</div>

                                            <div class="col-lg-10">
                                                <%=infliencerStatisticsArr.twentyFive_to_thirtyFour%>%
                                              
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-2">35 To 44</div>
                                            <div class="col-lg-10">
                                                <%=infliencerStatisticsArr.thirtyFive_to_fourtyFour%>%
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-lg-2">More than 44</div>
                                            <div class="col-lg-10">
                                                <%=infliencerStatisticsArr.moreThan_fourtyFour%>%
                                            </div>
                                        </div>
                                        <legend class="text-bold">
                                            Location of followers
                                        </legend>

                                        <% for(let i in infliencerStatisticsArr.locationstatisticsArr){ %>
                                        <div class="form-group">
                                            <div class="row"><div class="col-sm-6"><strong>Country</strong></div><div class="col-sm-6"><strong>Percentage</strong></div></div>
                                            <!-- <div class="col-lg-2">Country</div> -->
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].country_one%>
                                            </div>
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].country_one_percentage%>%
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <!-- <div class="col-lg-2">Country</div> -->
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].country_two%>
                                            </div>
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].country_two_percentage%>%
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <!-- <div class="col-lg-2">Country</div> -->
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].country_three%>
                                            </div>
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].country_three_percentage%>%
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-sm-6"><strong>City</strong></div>
                                                <div class="col-sm-6"><strong>Percentage</strong></div>
                                            </div>
                                            <!-- <div class="col-lg-2">Country</div> -->
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].city_one%>
                                            </div>
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].city_one_percentage%>%
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            
                                            <!-- <div class="col-lg-2">Country</div> -->
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].city_two%>
                                            </div>
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].city_two_percentage%>%
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            
                                            <!-- <div class="col-lg-2">Country</div> -->
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].city_three%>
                                            </div>
                                            <div class="col-lg-6">
                                                <%=infliencerStatisticsArr.locationstatisticsArr[i].city_three_percentage%>%
                                            </div>
                                        </div>
                                        
                                        

                                        
                                        <legend></legend>
                                        <% } %>
                                </form>

                                
                                    
                                <div class="text-right">
                                
                                <a href="/admin/influencer/statistics" style="background-color: #8080808c; color: black;" type="submit"
                                    class="btn btn-secondary">
                                    Back
                                </a>
                                </div>
                                
                                
                            </div>
                        </div>
                        <!-- Footer -->
                        <%-include('../partials/footer')%>
                            <!-- /footer -->

                    </div>
                    <!-- /content area -->
                </div>
                <!-- /main content -->
        </div>
        <!-- /page content -->
    </div>
    <!-- /page container -->

    </body>

    </html>